<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.empresa.bitwise_trabajo.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <TextField fx:id="number1" promptText="Enter first number"/>
  <TextField fx:id="number2" promptText="Enter second number"/>
  <Button text="AND" onAction="#onAndButtonClick"/>
  <Button text="OR" onAction="#onOrButtonClick"/>
  <Button text="XOR" onAction="#onXorButtonClick"/>
  <Button text="NOT" onAction="#onNotButtonClick"/>
  <Button text="LEFT SHIFT" onAction="#onLeftShiftButtonClick"/>
  <Button text="RIGHT SHIFT" onAction="#onRightShiftButtonClick"/>
  <Button text="UNSIGNED RIGHT SHIFT" onAction="#onUnsignedRightShiftButtonClick"/>
  <Label fx:id="resultText"/>
</VBox>